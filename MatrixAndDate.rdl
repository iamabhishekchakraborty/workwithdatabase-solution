  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorks2014</DataSourceName>
        <CommandText>SELECT      PC.Name AS ProdCat, PS.Name AS SubCat, DATEPART(yy, SOH.OrderDate) AS OrderYear, 
DATENAME(MONTH,SOH.OrderDate) AS OrderMonth,
(SOD.UnitPrice * SOD.OrderQty) AS Sales,
SOH.OrderDate
FROM        Production.ProductSubcategory PS 
INNER JOIN  Sales.SalesOrderHeader SOH 
INNER JOIN  Sales.SalesOrderDetail SOD ON SOH.SalesOrderID = SOD.SalesOrderID 
INNER JOIN  Production.Product P ON SOD.ProductID = P.ProductID 
            ON PS.ProductSubcategoryID =  P.ProductSubcategoryID INNER JOIN
            Production.ProductCategory PC ON PS.ProductCategoryID = PC.ProductCategoryID
WHERE DATEPART(yy, SOH.OrderDate) = '2007' 
GROUP BY    PC.Name, PS.Name,DATEPART(yy, SOH.OrderDate), DATENAME(MONTH,SOH.OrderDate),(SOD.UnitPrice * SOD.OrderQty),SOH.OrderDate</CommandText>
      </Query>
      <Fields>
        <Field Name="ProdCat">
          <DataField>ProdCat</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubCat">
